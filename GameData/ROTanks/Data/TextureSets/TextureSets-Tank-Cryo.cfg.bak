KSP_TEXTURE_SET
{
    name = ROT-Cryo-Stripes1-Stringers
    title = S1-S
    recolorable = true
    MATERIAL
    {
        shader = TU/Metallic
        texture = _MainTex, ROTanks/Assets/SC-TANKS-DIFF-WHITE-S
        texture = _BumpMap, ROTanks/Assets/SC-TANKS-NRM-S
        texture = _MetallicGlossMap, ROTanks/Assets/SC-TANKS-MET-STD
