@ROL_MODEL[0.25x-Cryo|0.5x-Cryo|0.75x-Cryo|1.0x-Cryo|1.5x-Cryo|2.0x-Cryo|2.5x-Cryo|3.0x-Cryo|3.5x-Cryo|4.0x-Cryo|4.5x-Cryo|5.0x-Cryo|5.5x-Cryo|6.0x-Cryo|6.5x-Cryo|7.0x-Cryo|7.5x-Cryo|8.0x-Cryo|0.25x-Kero|0.25x-Hydro|0.75x-Kero|0.75x-Hydro]:FOR[ROTanks]
{    
    //standard stripe patterns, with plain NRM map
    textureSet = ROT-Cryo-Stripes1-Plain
    textureSet = ROT-Cryo-Stripes2-Plain
    textureSet = ROT-Cryo-Stripes3-Plain
    textureSet = ROT-Cryo-Stripes6-Plain

    //special texture sets with custom NRM
    textureSet = ROT-Cryo-Soyuz
    textureSet = ROT-Cryo-Orange-Plain
    textureSet = ROT-Cryo-Orange-Stringers
    textureSet = ROT-Cryo-Gold
    textureSet = ROT-Cryo-Silver
}

@ROL_MODEL[8.0x-Cryo]:FOR[ROTanks]
{    
    //standard stripe patterns, with plain NRM map
    textureSet = ROT-Cryo-Pap
    textureSet = ROT-Cryo-Pap2
}
